/*image-sizes*/

.product-image {
  position: relative;
}
.image-wrap {
  img {
    position: absolute;
    top: 50%;
    left: 50%;
    height: 100%;
    width: 100%;
    -webkit-transform: translateX(-50%) translateY(-50%); 
    -moz-transform: translateX(-50%) translateY(-50%);
    -o-transform: translateX(-50%) translateY(-50%);
    transform: translateX(-50%) translateY(-50%);
    object-fit: cover;
  }
}
.image-fit-contain {
  .image-wrap {
    img {
      object-fit: contain;
    }
  }
}
.image-pos-top {
  .image-wrap {
    img {
      object-position: top;
    }
  }
}
.image-pos-bottom {
  .image-wrap {
    img {
      object-position: bottom;
    }
  }
}

.product-image-animation {
  .image-wrap {
    img {
      -webkit-transition: all .4s ease-in-out;
      -moz-transition: all .4s ease-in-out;
      -o-transition: all .4s ease-in-out;
      -ms-transition: all .4s ease-in-out;
      transition: all .4s ease-in-out;
      
    }  
  }
  .product-list-item:hover {
    img {
      -webkit-transform: translateX(-50%) translateY(-50%) scale(1.2); 
      -moz-transform: translateX(-50%) translateY(-50%) scale(1.2);
      -o-transform: translateX(-50%) translateY(-50%) scale(1.2);
      transform: translateX(-50%) translateY(-50%) scale(1.2);
    }
  }
  .not-animate .product-list-item:hover {
    img {
      -webkit-transform: translateX(0%) translateY(0%) scale(1.0); 
      -moz-transform: translateX(0%) translateY(0%) scale(1.0);
      -o-transform: translateX(0%) translateY(0%) scale(1.0);
      transform: translateX(0%) translateY(0%) scale(1.0);
    }
  }
}

.image-size-one-one {
  .image-wrap {
    padding-top: 100%;
  }
}
.image-size-three-two {
  .image-wrap {
    padding-top: calc((2 / 3) * 100%);
  }
}
.image-size-fout-three {
  .image-wrap {
    padding-top: calc((3 / 4) * 100%);
  }
}
.image-size-sixteen-nine {
