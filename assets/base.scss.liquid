  margin: 16px;
  width: 80%;
  display: flex;
  column-gap: 10px;
  flex-wrap: wrap;
  row-gap: 10px;
  z-index: 1;
  @media only screen and (min-width: 768px) {
  }
}
.right-banner {
  top: 0;
  position: absolute;
  z-index: 2;
  right: 0;
  width: 120px;
  height: 120px;
}
.badge {
  font-size: 8px;
  line-height: 14px;
  font-weight: 700;
  padding: 4px 12px;
  @media only screen and (min-width: 768px) {
    font-size: 12px;
    line-height: 18px;
    font-weight: 700;
    padding: 6px 16px;
  }
} 
.badge.model-1 {
  color: var(--badge-one-color);
  border: 1px solid var(--badge-one-color);
  
}
.badge.model-2 {
  color: var(--badge-two-color);
  border: 1px solid var(--badge-two-color);
}
.badge.model-3 {
  color: #fff;
  background-color: var(--badge-three-color);
}
.right-banner .badge.model-3 {
  color: #fff;
  background-color: var(--badge-three-color);
  rotate: 45deg;
  margin-right: -45%;
  margin-top: 10%;
}


